package com.FunctionDemos;

public class D05MethodOverloading {

	public static void calculateSum1(int n1, int n2)
	{
		int sum = n1 + n2;
		System.out.println("Sum: " + sum);
	}
	public static void calculateSum2(int n1, int n2, int n3)
	{
		int sum = n1 + n2 + n3;
		System.out.println("Sum: " + sum);
	}
	public static void calculateSum3(double n1, double n2)
	{
		double sum = n1 + n2;
		System.out.println("Sum: " + sum);
	}
	public static void calculateSum4(double n1, int n2)
	{
		double sum = n1 + n2;
		System.out.println("Sum: " + sum);
	}
	public static void calculateSum5(int n1, double n2)
	{
		double sum = n1 + n2;
		System.out.println("Sum: " + sum);
	}
	public static void main(String[] args) {
		calculateSum1(10, 20);
	}

}
